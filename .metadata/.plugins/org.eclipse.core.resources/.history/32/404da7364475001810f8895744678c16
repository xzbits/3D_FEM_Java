package oofem;

public class Constraint {
	private boolean[] free = new boolean[3];
	public Constraint (boolean u1, boolean u2, boolean u3) {
		this.free[1]=u1;
		this.free[2]=u2;
		this.free[3]=u3;
	}
	public boolean isFree(int c) {
		return this.free[c];
	}
	public void print() {
				if(free[0]=true) {
					System.out.println("free");}
				else {
					System.out.println("fixed");}
				
				if(free[1]=true) {
					System.out.println("free");}
				else {
					System.out.println("fixed");}
				
				if(free[2]=true) {
					System.out.println("free");}
				else {
					System.out.println("fixed");}
				}
}
